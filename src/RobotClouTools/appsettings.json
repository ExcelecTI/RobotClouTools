{
    "Kestrel": {
        "Endpoints": {
            "Http": { "Url": "http://127.0.0.1:8080" }
        }
    },
    "Tcp": {
        "BindAddress": "127.0.0.1",
        "Port": 9901,
        "Backlog": 200,
        "MaxConcurrentConnections": 200,
        "ReadTimeoutMs": 30000,
        "WriteTimeoutMs": 30000,
        "LineDelimiter": "\n",
        "MaxLineBytes": 32768
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    }
}
